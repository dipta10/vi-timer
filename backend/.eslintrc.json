{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json"
    ]
  },
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": "error",
    "import/no-unresolved": [
      0,
      {
        "caseSensitive": false
      }
    ]
  },
  "plugins": [
    "prettier"
  ]
}

